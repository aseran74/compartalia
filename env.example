# Configuración de APIs para Compartalia
# Google Places API - Para autocompletado de direcciones y geocodificación
VITE_GOOGLE_PLACES_API_KEY=tu_google_places_api_key_aqui

# Google Maps API - Para mostrar mapas y rutas
VITE_GOOGLE_MAPS_API_KEY=tu_google_maps_api_key_aqui

# OpenStreetMap (alternativa gratuita)
VITE_NOMINATIM_API_URL=https://nominatim.openstreetmap.org

# OpenRouteService (GRATIS - 2000 requests/día)
VITE_ORS_API_KEY=tu_openrouteservice_api_key_aqui

# Mapbox (opcional - para polylines avanzadas)
VITE_MAPBOX_ACCESS_TOKEN=tu_mapbox_access_token_aqui

# Configuración de la aplicación
VITE_APP_NAME=Compartalia
VITE_APP_DESCRIPTION=Sistema de Carpooling Inteligente
VITE_APP_VERSION=1.0.0

# Configuración de desarrollo
VITE_DEV_MODE=true
VITE_MOCK_API=true

# Configuración de notificaciones
VITE_FCM_SERVER_KEY=tu_fcm_server_key_aqui

# Configuración de chat en tiempo real (opcional)
VITE_PUSHER_APP_KEY=tu_pusher_app_key_aqui
VITE_PUSHER_APP_CLUSTER=eu
